	<queryString>
		<![CDATA[SELECT b.NAME,b.addl1,b.addl2, b.phone, a.ID,a.PO, a.dz_box, a.style, a.n_box, a.units_box, a.osgroup, a.shipto, a.total_box,a.invoice, a.color, a.v_style,a.g_id,
CASE a.units_box WHEN 0 THEN ' '
   ELSE concat('+ ',concat(a.units_box,' PCS')) END units_box2,
CASE  WHEN c.description is null THEN 'BELTS'
   ELSE c.description END description
FROM P_MAKELABELS a INNER JOIN CORP b ON a.CORP = b.ID
LEFT OUTER JOIN ITEMS C
ON a.style = c.code
WHERE a.po =$P{p_ponumber}  and a.g_id = $P{p_gid}

ORDER BY a.osgroup ASC]]>
	</queryString>
