	<queryString>
		<![CDATA[select c.name corp_name,
       c.addl1,
       c.addl2,
       c.phone,
       c.email,
       rpt_c.title,
       rpt_c.name_for,
       rpt_c.note,
       i.code,
       i.description,
       idet.color,
       idet.qty_units,
       idet.pic,
       rpt_c.id,
       rpt_d.id_item_det
from RPT_ITEMS_CTLG_DET rpt_d, rpt_items_ctlg rpt_c, items_det idet, items i, corp c
where rpt_d.id_item_det = idet.id
  and i.id = idet.item_fk
  and i.corp = $P{P_CORP}
  and rpt_d.corp = i.corp
  and c.id = i.corp
  and rpt_c.id = $P{P_RPTID}
  and rpt_d.id_rpt = rpt_c.id]]>
	</queryString>
