	<queryString>
		<![CDATA[SELECT b.NAME,b.addl1,b.addl2, b.phone, a.ID,a.PO, a.dz_box, a.style, a.n_box,a.invoice,
CASE a.units_box WHEN 0 THEN ' '
   ELSE concat(a.units_box,' PCS') END units_box2,
CASE a.po WHEN '-' THEN ' '
   ELSE concat('PO: ', a.po) END po_number,
a.osgroup, a.shipto, a.total_box, a.color
FROM P_MAKELABELS a INNER JOIN CORP b ON a.CORP = b.ID
WHERE a.po =$P{p_ponumber}  and a.g_id = $P{p_gid}
ORDER BY a.osgroup ASC]]>
	</queryString>
