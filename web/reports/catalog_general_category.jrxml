	<queryString>
		<![CDATA[select c.name, c.addl1, c.addl2, c.phone, c.email, i.code,
ic.category,
i.description,
idet.color,
idet.i_size,
idet.qty_units,
idet.pic,
idet.pic_name,
case idet.gender
when 'M' then 'Men'
when 'W' then 'Women'
when 'KG' then 'Girls'
when 'KB' then 'Boys'
when 'U' then 'Unisex'
end as gender
from items i, items_det idet, items_category ic, corp c
where i.id = idet.item_fk
  and i.categoryfk = ic.id
  and idet.gender in ($P{P_GENDER},$P{P_UNISEX})
  and ic.id = $P{P_IDCATEGORY}
  and c.id = $P{P_CORP}
  and c.id = idet.corp
  and idet.corp = i.corp
order by code, idet.gender asc]]>
	</queryString>
